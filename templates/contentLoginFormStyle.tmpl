
.leo-auth-form *{
	letter-spacing: normal !important;
	margin: 0 !important;
    padding: 0 !important;
	background: none !important;
	border: 0 !important;
	float: none !important;
	text-align: left !important;
	text-decoration: none !important;
    font: normal 15px 'Lucida Grande', 'Lucida Sans Unicode', Lucida, Arial, Helvetica, sans-serif !important;
}

.leo-auth-form {
    background-color: #fff !important;
    width: 280px !important;
    padding: 0 0 25px !important;
    color: #333333 !important;
    font: 13px "Lucida Sans Unicode", "Myriad pro", Arial !important;
    overflow: hidden !important;
    position: absolute !important;
    border: 1px solid #dddddd !important;
    z-index: 9999999 !important;
    box-sizing: content-box !important;
    border-radius: 8px !important;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5) !important;
}
    .leo-auth-form fieldset {
        padding: 0 0 0 30px !important;
        margin: 0 !important;
        border: none !important;
    }
        .leo-auth-close {
            position: absolute !important;
            right: 9px !important;
            top: 9px !important;
            cursor: pointer !important;
        }

        .leo-auth-form h4 {
            text-align: left !important;
            color: #333 !important;
            margin: 0 0 17px !important;
            font: 13px/16px "Lucida Sans Unicode", "Myriad pro", Arial !important;
            background-color: #e8edf5 !important;
            padding: 7px 25px 7px 30px !important;
            border-radius: 8px 8px 0 0 !important;
            border-bottom: 1px solid #d6dae2 !important;
            background: -moz-linear-gradient(left, #eaeff6 50%, #e8edf5 50%) !important;
            background: -webkit-linear-gradient(left, #eaeff6 50%, #e8edf5 50%) !important;
            background: -o-linear-gradient(left, #eaeff6 50%, #e8edf5 50%) !important;
            background: -ms-linear-gradient(left, #eaeff6 50%, #e8edf5 50%) !important;
            background:  linear-gradient(left, #eaeff6 50%, #e8edf5 50%) !important;
            background-size: 8px 8px !important;
            box-sizing: content-box !important;
        }

        .leo-auth-form dl {
            margin: 0 0 16px 0 !important;
            width: 220px !important;
        }
            .leo-auth-form dt {
                zoom: 1 !important;
                margin: 0 0 7px !important;
                padding: 0 !important;
                text-align: left !important;
            }

                .leo-auth-form dt a {
                    font: 13px "Lucida Sans Unicode", "Myriad pro", Arial !important;
                    font-size: 11px !important;
                    float: right !important;
                    font-weight: normal !important;
                    text-decoration: underline !important;
                    line-height: normal !important;
                }
                .leo-auth-new-account {
                    color: #3f69a9 !important;
                }
                .leo-auth-form .leo-auth-new-account:hover {
                    color: #639b54 !important;
                }
                .leo-auth-forgot-pass{
                    color: #666666 !important;
                }
                .leo-auth-form .leo-auth-forgot-pass:hover {
                    color: #000000 !important;
                }
                .leo-auth-form label {
                    font: 13px "Lucida Sans Unicode", "Myriad pro", Arial !important;
                    margin: 0 0 7px !important;
                    padding: 0 !important;
                    width: auto !important;
                    float: left !important;
                    text-align: left !important;
                    display: inline !important;
                }

            .leo-auth-form dd {
                padding: 0 !important;
                margin: 0 !important;
            }

                .leo-auth-input{
                    margin: 0 !important;
                    background: #fff !important;
                    width: 210px !important;
                    _width: 218px !important;
                    padding: 0 4px !important;
                    height: 34px !important;
                    border: 1px solid #dddddd !important;
                    font: 13px/34px "Lucida Sans Unicode", "Myriad pro", Arial !important;
                    border-top-color: #9a9a9a !important;
                    border-radius: 3px !important;
                    box-shadow: inset 0 1px 0 rgba(0, 0, 0, 0.1) !important;
                    box-sizing: content-box !important;
                }
                .leo-auth-input:focus{
                    background-color: #fffec9 !important;
                }

                .leo-auth-form p {
                    font-size: 11px !important;
                    padding: 0 !important;
                    position: relative !important;
                    margin:-5px 0 0 !important;
                    top: 6px !important;
                }
                .sign-in {
                    font: 13px "Lucida Sans Unicode", "Myriad pro", Arial !important;
                    cursor: pointer !important;
                    display: block !important;
                    line-height: 1 !important;
                    height: 35px !important;
                    vertical-align: baseline !important;
                    width: 110px !important;
                    font-size: 15px !important;
                    margin: -2px 0 0 0 !important;
                    text-decoration: none !important;
                    text-align: center !important;
                    color: #fff !important;
                    text-shadow: 0 1px 0 rgba(0, 0, 0, 0.2) !important;
                    border: 1px solid #5e86c4 !important;
                    border-bottom-color: #325386 !important;
                    background-color: #3f69a9 !important;
                    background-repeat: repeat-x !important;
                    background-image: -moz-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: -webkit-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: -o-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: linear-gradient(top, #5c84c2, #3f69a9) !important;
                    border-radius: 2px !important;
                    box-shadow: 0 0 1px rgba(34, 57, 91, 0.5), 0 2px 1px -2px rgba(255, 255, 255, 0.9) inset !important;
                }
                input::-moz-focus-inner {
                    border: 0 !important;
                    padding: 0 !important;
                }
                .leo-auth-form input[type="submit"]:hover {
                    background-color: #4675bd !important;
                    background-repeat: repeat-x !important;
                    background-image: -moz-linear-gradient(top, #6592d8, #4675bd) !important;
                    background-image: -webkit-linear-gradient(top, #6592d8, #4675bd) !important;
                    background-image: -o-linear-gradient(top, #6592d8, #4675bd) !important;
                    background-image: linear-gradient(top, #6592d8, #4675bd) !important;
                }
                .leo-auth-form input[type="submit"]:active {
                    background-color: #3f69a9 !important;
                    background-repeat: repeat-x !important;
                    background-image: -moz-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: -webkit-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: -o-linear-gradient(top, #5c84c2, #3f69a9) !important;
                    background-image: linear-gradient(top, #5c84c2, #3f69a9) !important;
                    box-shadow: 0 0 1px rgba(34, 57, 91, 0.5), 0 2px 1px -2px rgba(0, 0, 0, 0.3) inset !important;
                    border-bottom-color: #3f69a9 !important;
                }

                .leo-auth-form .icon-close {
                    background-position: 2px 2px !important;
                    width: 13px !important;
                    height: 13px !important;
                    display: block !important;
                    background-image: url({llImagesUrl}/icons.png) !important;
                }

                .leo-auth-form .icon-close:hover {
                    background-position: -9px 2px !important;
                }

                .leo-auth-form .icon-paw {
                    width: 16px !important;
                    height: 18px !important;
                    background-position: left -16px !important;
                    display: block !important;
                    background-image: url({llImagesUrl}/icons.png) !important;
                    position: absolute !important;
                    left: 8px !important;
                    top: 6px !important;
                }

.leo-auth-form-error {
    color: #B4211B !important;
}



